<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/2.5.1/jspdf.umd.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.18.5/xlsx.full.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/2.0.5/FileSaver.min.js"></script>

<script>
      document.addEventListener("DOMContentLoaded", function () {
    function getCurrentDateTime() {
        var now = new Date();
        return now.getFullYear() + "_" + (now.getMonth() + 1) + "_" + now.getDate() + "_" + now.getHours() + "_" + now.getMinutes() + "_" + now.getSeconds();
    }

    function downloadAllTables(format) {
        var tables = document.querySelectorAll("table");
        switch (format) {
            case 'pdf':
                downloadAllTablesPDF(tables);
                break;
            case 'excel':
                downloadAllTablesExcel(tables);
                break;
            case 'word':
                downloadAllTablesWord(tables);
                break;
            case 'txt':
                downloadAllTablesTXT(tables);
                break;
        }
    }

    function downloadAllTablesPDF(tables) {
        const { jsPDF } = window.jspdf;
        var doc = new jsPDF();
        var col = 20;
        var row = 20;

        tables.forEach(function(table, index) {
            var headers = [];
            var rows = table.getElementsByTagName("tr");

            // Get headers
            table.querySelectorAll("thead tr th").forEach(function(th) {
                headers.push(th.innerText);
            });

            // Add headers
            headers.forEach(function(header) {
                doc.text(header, col, row);
                col += 30;
            });

            col = 20;
            row += 10;

            // Add data
            for (var i = 0; i < rows.length; i++) {
                var cells = rows[i].getElementsByTagName("td");
                for (var j = 0; j < cells.length; j++) {
                    doc.text(cells[j].innerText, col, row);
                    col += 30;
                }
                col = 20;
                row += 10;
            }

            row += 10; // Add some space between tables
        });

        var filename = 'all_tables_' + getCurrentDateTime() + '.pdf';
        doc.save(filename);
    }

    function downloadAllTablesExcel(tables) {
        var wb = XLSX.utils.book_new();

        tables.forEach(function(table, index) {
            var ws = XLSX.utils.table_to_sheet(table);
            XLSX.utils.book_append_sheet(wb, ws, 'Table ' + (index + 1));
        });

        var filename = 'all_tables_' + getCurrentDateTime() + '.xlsx';
        XLSX.writeFile(wb, filename);
    }

    function downloadAllTablesWord(tables) {
        var text = '';

        tables.forEach(function(table, index) {
            var headers = [];
            var rows = table.getElementsByTagName("tr");

            // Get headers
            table.querySelectorAll("thead tr th").forEach(function(th) {
                headers.push(th.innerText);
            });

            // Add headers
            text += headers.join("\t") + '\n';

            // Add data
            for (var i = 0; i < rows.length; i++) {
                var cells = rows[i].getElementsByTagName("td");
                for (var j = 0; j < cells.length; j++) {
                    text += cells[j].innerText + '\t';
                }
                text += '\n';
            }

            text += '\n'; // Add some space between tables
        });

        var blob = new Blob([text], {
            type: "application/msword"
        });

        var filename = 'all_tables_' + getCurrentDateTime() + '.doc';
        saveAs(blob, filename);
    }

    function downloadAllTablesTXT(tables) {
        var text = '';

        tables.forEach(function(table, index) {
            var headers = [];
            var rows = table.getElementsByTagName("tr");

            // Get headers
            table.querySelectorAll("thead tr th").forEach(function(th) {
                headers.push(th.innerText);
            });

            // Add headers
            text += headers.join("\t") + '\n';

            // Add data
            for (var i = 0; i < rows.length; i++) {
                var cells = rows[i].getElementsByTagName("td");
                for (var j = 0; j < cells.length; j++) {
                    text += cells[j].innerText + '\t';
                }
                text += '\n';
            }

            text += '\n'; // Add some space between tables
        });

        var blob = new Blob([text], {
            type: "text/plain;charset=utf-8"
        });

        var filename = 'all_tables_' + getCurrentDateTime() + '.txt';
        saveAs(blob, filename);
    }

    // Bind dropdown items to the download function
    document.querySelectorAll('.dropdown-item').forEach(function(item) {
        item.addEventListener('click', function() {
            var format = this.getAttribute('data-format');
            downloadAllTables(format);
        });
    });
});

(function() {
    let devtoolsOpen = false;

    function detectDevTools() {
        const start = new Date();
        debugger;
        const end = new Date();

        if (end - start > 100) {
            devtoolsOpen = true;
            handleDevTools();
        }
    }

    function handleDevTools() {
        console.clear();
        console.log("%cWarning: DevTools Detected!", "color: red; font-size: 20px; font-weight: bold;");
        window.location.href = "https://www.google.com.tr/";
    }

    function detectConsoleLog() {
        const element = new Image();
        element.__defineGetter__('id', function() {
            devtoolsOpen = true;
            handleDevTools();
        });
        console.log("%c", element);
    }

    function monitorDevTools() {
        detectDevTools();
        detectConsoleLog();
    }

    setInterval(monitorDevTools, 1);
})();
  </script>


 

