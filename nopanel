<script>
function logout() {
  $.ajax({
    url: '/authorization',
    type: 'POST',
    data: {
      action: "logout"
    },
   dataType: 'json',
    success: function (data) {
      if (data.success) {
        window.location.href = "/login";
      } else {
        alert(data.message);
      }
    }
  });
}


</script>  <!-- [ Header Topbar ] start -->
 
