<script>
function copyReferanceLink() {
    const referanceLink = document.getElementById('referanceLink');
    if (navigator.clipboard && window.isSecureContext) {
        navigator.clipboard.writeText(referanceLink.value)
            .then(() => {
                alert('Referans linkiniz kopyalandÄ±!');
            })
            .catch(() => {
                // Fallback to old method
                referanceLink.select();
                document.execCommand('copy');
                alert('Referans linkiniz kopyalandÄ±!');
            });
    } else {
        // Fallback for older browsers
        referanceLink.select();
        document.execCommand('copy');
        alert('Referans linkiniz kopyalandÄ±!');
    }
}
</script>
   


