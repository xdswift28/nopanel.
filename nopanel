    function closemodal(url){
        $('#reklammodal').modal('hide');
        setCookie('modal_closed', 'true', 10);
        window.open(url, '_blank');
    }

    function opennewtab(url){
        $('#reklammodal').modal('hide');
        setCookie('modal_closed', 'true', 10);
        window.open(url, '_blank');
    }

    $(document).ready(function(){
        if('true' !== 'false') {
            $('#reklammodal').modal('show');
        }
    });

    function readNotification(id) {
        $.ajax({
            url: '/authorization',
            type: 'POST',
            data: {
                id: id,
                action: 'readNotification'
            },
            dataType: 'json',
            success: function(response) {
              if (response.success) {
                location.reload();
              }
            }
        });
    }

    function readAllNotifications() {
        $.ajax({
            url: '/authorization',
            type: 'POST',
            data: {
                action: 'readAllNotifications'
            },
            dataType: 'json',
            success: function(response) {
              if (response.success) {
                location.reload();
              }
            }
        });
    }

    function deleteNotification(id) {
        $.ajax({
            url: '/authorization',
            type: 'POST',
            data: {
                id: id,
                action: 'deleteNotification'
            },
            dataType: 'json',
            success: function(response) {
              if (response.success) {
                location.reload();
              }
            }
        });
    }

    function deleteAllNotifications() {
        $.ajax({
            url: '/authorization',
            type: 'POST',
            data: {
                action: 'deleteAllNotifications'
            },
            dataType: 'json',
            success: function(response) {
              if (response.success) {
                location.reload();
              }
            }
        });
    }


</script>

  
